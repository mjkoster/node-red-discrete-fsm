        "id": "d1174de12deed6a7",
        "type": "function",
        "z": "da1da3acd2b01b0a",
        "name": "Cond_1",
        "func": "if (msg.topic == \"input\" && eval(node.condition)) {\n  msg.topic = \"condition\";\n  msg.payload = node.name;\n  return msg; \n  }",
        "outputs": 1,
        "noerr": 0,
        "initialize": "// Code added here will be run once\n// whenever the node is started.\nnode.condition = \"flow.In_1==1 && flow.In_2==1 && flow.In_3==0\";\n",
