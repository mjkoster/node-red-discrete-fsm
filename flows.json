    {
        "id": "117b647b4fd5038d",
        "type": "function",
        "z": "da1da3acd2b01b0a",
        "name": "State_1",
        "func": "if (msg.topic == \"condidtion\") {\n    if (true) {\n        msg.topic = \"transition\";\n        msg.payload = node.transitions[msg.payload];\n        return msg;\n    }\n}\nif (msg.topic == \"transition\" && msg.payload == node.name) {\n    flow.state = node.name;\n    flow.outputs = node.outputs;\n    msg.topic = \"output\";\n    msg.payload = node.outputs;\n    return msg;\n}",
        "outputs": 1,
        "noerr": 0,
        "initialize": "// Code added here will be run once\n// whenever the node is started.\nnode.initialState = true;\nif (node.initialState) {\n    flow.state = node.name;\n}\nnode.transitions = {\n    \"Cond_1\": \"State_2\",\n    \"Cond_2\": \"State_3\"\n}\nnode.outputs = {\n    \"Out_1\": 0,\n    \"Out_2\": 0,\n    \"Out_3\": 1\n}",
        "finalize": "",
        "libs": [],
        "x": 580,
        "y": 240,
